          <CommandParameters>
            <Parameter DataField="EventID" Type="System.String" />
          </CommandParameters>
          <Fields>
            <DaField DataField="Math" FillType="String" />
            <DaField DataField="Chinese"  FillType="String" />
          </Fields>
        </DataSouce>
        
        <!--*******普通表格*******-->
      </DataSet>
    </Data>
