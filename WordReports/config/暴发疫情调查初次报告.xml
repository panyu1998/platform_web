		    <!--症状比率-->
        <DataSouce SqlString="exec sp_GetSymptomRate @EventID" Name="2" BookMark="SymptomRate">
          <CommandParameters>
            <Parameter DataField="EventID" Type="System.String" />
          </CommandParameters>
          <Fields>
            <DaField DataField="Math" FillType="String" />
            <DaField DataField="Chinese"  FillType="String" />
          </Fields>
        </DataSouce>
		    <!--上报单位名称和地址及其他基本信息-->
